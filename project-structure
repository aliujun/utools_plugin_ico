png-to-ico/
  ├── src/
  │   ├── components/
  │   │   └── ImageConverter.vue    # 图片转换组件
  │   ├── App.vue                   # 主应用组件
  │   ├── main.js                   # Vue入口文件
  │   └── imageUtils.js             # 图片处理工具
  ├── dist/                         # 构建输出目录
  ├── index.html                    # HTML入口
  ├── plugin.json                   # 插件配置
  ├── preload.js                    # 预加载脚本
  ├── vite.config.js               # Vite配置
  ├── build.js                     # 打包脚本
  └── logo.png                     # 插件图标 